# Fail2ban filter for Remember Me API
# Generated by Ansible

[Definition]
# Failed authentication attempts
failregex = ^.*\[ERROR\].*Failed login attempt.*from <HOST>.*$
            ^.*\[WARN\].*Invalid API key.*from <HOST>.*$
            ^.*\[ERROR\].*Unauthorized access attempt.*from <HOST>.*$
            ^.*401 Unauthorized.*"remote_ip":"<HOST>".*$

# Ignore internal health checks
ignoreregex = ^.*\/health.*$